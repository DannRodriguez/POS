<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="325.0" prefWidth="769.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="serviciosocial.CodesController">
   <children>
      <StackPane layoutX="33.0" layoutY="27.0" prefHeight="271.0" prefWidth="703.0" styleClass="background-pane" stylesheets="@style.css">
         <children>
            <VBox prefHeight="297.0" prefWidth="663.0">
               <StackPane.margin>
                  <Insets bottom="100.0" />
               </StackPane.margin>
               <children>
                  <Label text="Ingrese su código de activación">
                     <VBox.margin>
                        <Insets bottom="10.0" left="190.0" top="30.0" />
                     </VBox.margin>
                     <font>
                        <Font name="Berlin Sans FB" size="24.0" />
                     </font>
                  </Label>
                  <TextField fx:id="CodeField" alignment="TOP_CENTER" prefHeight="67.0" prefWidth="554.0" promptText="XXXX - XXXX - XXXX - XXXX" styleClass="idusuario3" stylesheets="@style.css">
                     <VBox.margin>
                        <Insets left="50.0" right="50.0" />
                     </VBox.margin>
                  </TextField>
               </children>
            </VBox>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="100.0">
               <StackPane.margin>
                  <Insets top="170.0" />
               </StackPane.margin>
               <children>
                  <Button fx:id="BtnRegisterCode" mnemonicParsing="false" styleClass="botoninicio3" stylesheets="@style.css" text="Registrar codigo">
                     <HBox.margin>
                        <Insets left="140.0" />
                     </HBox.margin>
                  </Button>
                  <Button fx:id="CloseBtn" mnemonicParsing="false" prefHeight="40.0" prefWidth="142.0" styleClass="botoninicio2" text="Cancelar" />
               </children>
            </HBox>
         </children>
      </StackPane>
   </children>
</AnchorPane>
